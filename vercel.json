{
  "version": 2,
  "installCommand": "npm install && composer install --no-dev --optimize-autoloader",
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "public/index.php"
    }
  ],
  "functions": {
    "public/index.php": {
      "runtime": "vercel-php@0.6.0" 
    }
  },
  "outputDirectory": "public" 
}